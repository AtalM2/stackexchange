<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%-5p [%t] %c{1}: %m%n"/>
    </Console>
  </appenders>
  <loggers>
    <logger name="fr.univnantes.atal.stackexchange" level="info"/>
    <root level="warn">
      <appender-ref ref="Console"/>
    </root>
  </loggers>
</configuration>
